target "Orbit"

with source, requirements

location "https://download.eclipse.org/tools/orbit/simrel/orbit-aggregation/2024-06" {
	com.google.guava [33.0.0,34.0.0)
	org.junit [4,5)
	org.kohsuke.args4j [2.37.0,2.38.0)
	org.apache.commons.lang3 lazy
}

maven antlr
	scope = compile
	dependencyDepth = none
	missingManifest = error
	includeSources {
	
	dependency {
		groupId = "org.antlr"
		artifactId = "antlr4-runtime"
		version = "4.7.2"
	}
	
}