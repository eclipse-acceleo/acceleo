get-view "Package Explorer" | get-tree | select "test/src/test/otherModule.mtl" | double-click
with [get-editor "otherModule.mtl" | get-text-viewer] {
    set-caret-pos 3 33
    key-type BackSpace
    key-type "M1+s"
}
get-view Problems | get-tree | select "Errors \\(3 items\\)" | double-click
with [get-view Problems | get-tree] {
    get-item -path "Errors \\(3 items\\)" | get-item 
        -path "Couldn't find the 'myOtherTemplate\\(EClassifier=EClassifier\\)' service" -index 1 
        | get-property "values['Description']" 
        | equals "Couldn't find the 'myOtherTemplate(EClassifier=EClassifier)' service" | verify-true
    get-item -path "Errors \\(3 items\\)" | get-item 
        -path "Couldn't find the 'myOtherTemplate\\(EClassifier=EClassifier\\)' service" -index 1 
        | get-property "values['Resource']" | equals "test.mtl" | verify-true
    get-item -path "Errors \\(3 items\\)" | get-item 
        -path "Couldn't find the 'myOtherTemplate\\(EClassifier=EClassifier\\)' service" -index 1 
        | get-property "values['Path']" | equals "/test/src/test" | verify-true
    get-item -path "Errors \\(3 items\\)" | get-item 
        -path "Couldn't find the 'myOtherTemplate\\(EClassifier=EClassifier\\)' service" -index 1 
        | get-property "values['Location']" | equals "line 11" | verify-true
    get-item -path "Errors \\(3 items\\)" | get-item 
        -path "Couldn't find the 'myOtherTemplate\\(EClassifier=EClassifier\\)' service" -index 1 
        | get-property "values['Type']" | equals "Language Servers" | verify-true
}
