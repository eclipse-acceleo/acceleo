<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.eclipse.acceleo.aql.launcher.AcceleoLauncher"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.eclipse.acceleo.aql.launcher.AcceleoLauncher">
         </run>
      </application>
   </extension>
   <extension
         id="AcceleoLauncherProduct"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.acceleo.aql.launcher.AcceleoLauncher"
            name="Acceleo">
         <property
               name="appName"
               value="Acceleo">
         </property>
      </product>
   </extension>

</plugin>
