[module myModule('http://www.eclipse.org/emf/2002/Ecore')/]

[comment @main /]
[template public myTemplate(myParam : ecore::EPackage)]
  [file (myParam.name + '.txt', overwrite)]
    [for (myVariable : ecore::EClassifier | myParam.eClassifiers) separator(', ')]
      [myVariable.name/]
    [/for]
  [/file]
[/template]