[module multiLinesWithIndentationEndsWithNewLine('http://www.eclipse.org/emf/2002/Ecore')/]

[comment @main /]
[template public myTemplate(myParam : ecore::EPackage)]
  [file (myParam.name + '.txt', overwrite)]
    first text.
    - [myParam.name + '\n' + myParam.name + '\n' + myParam.name + '\n'/]
    last text.
  [/file]
[/template]