[module ifStatementSecondElseIf('http://www.eclipse.org/emf/2002/Ecore')/]

[template public myTemplate(myParam : ecore::EPackage)]
  [file (myParam.name + '.txt', overwrite)]
    [if (false)]
            then block.
    [elseif (false)]
            first elseif block.
    [elseif (true)]
            second elseif block.
    [else]
            else block.
    [/if]
  [/file]
[/template]