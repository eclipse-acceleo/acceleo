    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="container" nsURI="http://www.eclipse.org/acceleo/container"
    nsPrefix="container">
  <eClassifiers xsi:type="ecore:EClass" name="Abstract" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="Contained" abstract="true"/>
